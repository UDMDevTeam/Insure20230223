                                                            BorderThickness="1,1,1,1"
                                                            CornerRadius="4,4,4,4"
                                                            SnapsToDevicePixels="True">
                                                            <Border
                                                                Name="tabCenterFill"
                                                                Padding="12,2,12,2"
                                                                Background="{TemplateBinding Background}"
                                                                BorderBrush="Gray"
                                                                BorderThickness="1,1,1,1"
                                                                CornerRadius="3,3,3,3">
                                                                <!--  {TemplateBinding Padding}  -->
