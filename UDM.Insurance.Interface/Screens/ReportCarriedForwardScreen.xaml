                        IsEnabled="False"
                        Style="{DynamicResource INFlatButton}"
                        TabIndex="0" />

                    <Button
                        x:Name="btnBaseReport"
                        Grid.Row="16"
                        Grid.RowSpan="2"
                        Grid.Column="16"
                        Grid.ColumnSpan="4"
                        Margin="0,0,10,25"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        Click="btnBaseReport_Click"
                        Content="Base Report"
                        IsEnabled="False"
                        Style="{DynamicResource INFlatButton}"
                        TabIndex="0" />

                    <Button
                        x:Name="btnUpgradeReport"
                        Grid.Row="16"
                        Grid.RowSpan="2"
                        Grid.Column="20"
                        Grid.ColumnSpan="4"
                        Margin="0,0,15,25"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        Click="btnUpgradeReport_Click"
                        Content="Upgrade Report"
                        IsEnabled="False"
                        Style="{DynamicResource INFlatButton}"
                        TabIndex="0" />


                    <Button
                        x:Name="btnIGReport"
                        Grid.Row="16"
                        Grid.RowSpan="2"
                        Grid.Column="24"
                        Grid.ColumnSpan="4"
                        Margin="0,0,15,25"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        Click="btnIGReport_Click"
                        Content="IG Report"
                        IsEnabled="False"
                        Style="{DynamicResource INFlatButton}"
                        TabIndex="0" />

                    <Viewbox
                        Grid.Row="1"
                        Grid.RowSpan="4"
                        Grid.Column="13"
                        Grid.ColumnSpan="4"
                        Width="150"
                        Height="150"
                        Margin="0,0,22,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Stretch="Fill">
                        <igWPF:XamMonthCalendar
                            x:Name="calToDate"
                            Margin="0"
                            FontSize="16"
                            SelectedDatesChanged="calToDate_SelectedDatesChanged"
                            Style="{DynamicResource INXamMonthCalendarStyle2}"
                            TabIndex="0"
                            Visibility="Visible" />
                    </Viewbox>

                    <Button
                        x:Name="btnLoadLeads"
                        Grid.Row="3"
                        Grid.Column="25"
                        Grid.ColumnSpan="3"
                        Width="115"
                        Margin="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Click="btnLoadLeads_Click"
                        Content="Load Leads"
                        IsEnabled="True"
                        Style="{DynamicResource INFlatButton}"
                        TabIndex="0" />

                    <igWPF:XamDataGrid
                        x:Name="dgConfirmationStats"
                        Grid.Row="5"
                        Grid.RowSpan="12"
                        Grid.Column="7"
                        Grid.ColumnSpan="21"
