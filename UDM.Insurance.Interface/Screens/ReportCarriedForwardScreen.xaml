                        VerticalAlignment="Bottom"
                        Style="{DynamicResource INLabelText2}"
                        Text="To"
                        TextWrapping="NoWrap"
                        Visibility="Visible" />
                    <Button
                        x:Name="btnExportToExcel"
                        Grid.Row="4"
                        Grid.Column="25"
                        Grid.ColumnSpan="3"
                        Width="115"
                        Margin="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Click="btnExportToExcel_Click"
                        Content="Export"
                        Style="{DynamicResource INFlatButton}"
                        TabIndex="0" />


                </Grid>
            </Viewbox>
