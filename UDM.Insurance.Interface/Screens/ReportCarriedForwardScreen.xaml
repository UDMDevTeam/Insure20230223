                        HorizontalAlignment="Left"
                        VerticalAlignment="Bottom" />

                    <Button
                        x:Name="btnCombinedReport"
                        Grid.Row="16"
                        Grid.RowSpan="2"
                        Grid.Column="12"
                        Grid.ColumnSpan="4"
