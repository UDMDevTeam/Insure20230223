                                                            <RowDefinition x:Name="gridRow0" Height="Auto" />
                                                            <RowDefinition x:Name="gridRow1" Height="*" />
                                                        </Grid.RowDefinitions>
                                                        <DockPanel
                                                            x:Name="PART_HeaderArea"
                                                            Grid.Row="0"
                                                            Grid.Column="0"
                                                            Panel.ZIndex="1">
                                                            <ContentPresenter
                                                                x:Name="PreTabItemContent"
                                                                Margin="5,0,10,0"
                                                                Content="{TemplateBinding PreTabItemContent}"
